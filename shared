@echo off

:: Set the share path and drive letter
set sharePath=\\192.168.20.221\change-code
set driveLetter=X:

:: Set the username and password
set username=warehouse
set password=S6riTM1a29St0zw

:: Map the network drive
net use %driveLetter% %sharePath% /user:%username% %password% > nul 2>&1

:: Check if the drive was mapped successfully
if %errorlevel% equ 0 (
    echo Drive %driveLetter% mapped successfully.
) else (
    echo Failed to map drive %driveLetter%.
)

:: Pause to keep the command window open
pause
